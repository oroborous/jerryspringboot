<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow
      	https://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

    <view-state id="course-list">
        <transition on="register" to="register"></transition>
        <transition on="failure" to="register-failure"></transition>
    </view-state>

    <view-state id="register"></view-state>

    <view-state id="register-failure"></view-state>

</flow>